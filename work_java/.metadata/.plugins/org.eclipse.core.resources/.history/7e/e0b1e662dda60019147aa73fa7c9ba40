package com.prac1;

public class Monitor {

	public static void main(String[] args) {
		String name = "Ethereum";
		double weight = 0.021914;
		double price = -8.28;
		double display = 11364.66;
		String made_in = "삼성전자(주)";
		String number = "123-456-A";
		String ratio = "16:9";	
		boolean CD = true;
		String b = "";
		if (CD == true)
			b = "YES";
		else 
			b = "NO";
			
		System.out.println("코인이름:"+name+"\n마지막체결가:"+weight+"BTC\n전일대비:"
				+price+"%\n24시간거래량:"+display+" cm\n제조사:"
				+made_in+"\n모델번호: "+number+"\n화면비율: "+ratio
				+"\nCD매뉴얼: "+b);
		
		

	}

}
