package com.basic;

public class OperatorTest2 { //소스코드에 복사+붙여넣기하면 소스코드2 생성

	public static void main(String[] args) {
		//논리연산자(논리형 대이터 대상)
		boolean a = true, b = false, c= true;
		System.out.println(a & b);
		System.out.println(a | b);
		System.out.println(a ^ b); //xor
		
		boolean f = true;
		System.out.println(!f); //not

	}

}
