package com.basic;

public class LoopTest2 {

	public static void main(String[] args) {
		// for
		for(int i = 0; i < 5; ++i) { //조건식에서 i++ 나 ++i나 동일하게 작동, 초기값에서 타입 표기해줘야함
			for(int j = 0; j < 3; j++) {
				System.out.println(i + "," + j);
			}
			System.out.println(i + ". hello");
		}
		System.out.println("-----------------------------");
	}

}
