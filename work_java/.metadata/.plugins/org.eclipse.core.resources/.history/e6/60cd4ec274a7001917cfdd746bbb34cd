package com.basic;

public class MethodTest {
	//main메서드바깥에 다른 메서드(add)를 "정의"
	static int add(int a, int b) { 
		int c = a + b;
		return c;
	}
	//increase(): int형 변수 1개를 입력 받아 10을 더한 후 출력해 주는 메소드
	
	
	public static void main(String[] args) { //public static는 메서드의 수식어
		// main메서드 내부에서 사용자 정의 메소드(add)를 사용하는 부분
		int result = add(3, 8);
		System.out.println(result);
	}
	//main바깥
	
}
