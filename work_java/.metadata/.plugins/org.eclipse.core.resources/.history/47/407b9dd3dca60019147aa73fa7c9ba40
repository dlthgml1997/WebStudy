package com.prac1;

public class Monitor {

	public static void main(String[] args) {
		String name = "삼성 27형 커브드 모니터";
		double weight = 3.6;
		int price = 198000;
		double display = 68.5;
		String made_in = "삼성전자(주)";
		String number = "123-456-A";
		String ratio = "16:9";	
		boolean CD = true;
		String b = "";
		if (CD == true)
			b = "YES";
		else 
			b = "NO";
			
		System.out.println("제품명:"+name+"\n무게:"+weight+"kg\n가격:"
				+price+"\n디스플레이:"+display+" cm\n제조사:"
				+made_in+"\n모델번호: "+number+"\n화면비율: "+ratio
				+"\nCD매뉴얼: "+b);
		
		

	}

}
