package com.basic;

public class SwitchTest {

	public static void main(String[] args) {
		int score = 80;
		
		switch (score) { //switch + <컨트롤+스페이스>단축키
			//탭키 오른쪽, Shift+탭 왼쪽
			case 90:
				System.out.println("level-1");
				break;
			case 80:
				System.out.println("level-2");
				//break;
			case 70:
				System.out.println("level-3");
				break;
			default:
				System.out.println("no level");
				break;
		} 

	}

}
