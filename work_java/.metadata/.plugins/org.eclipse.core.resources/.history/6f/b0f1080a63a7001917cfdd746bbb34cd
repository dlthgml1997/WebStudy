package com.basic;

public class LoopTest2 {

	public static void main(String[] args) {
		// 여러겹의 for -- logic이 생각이 잘안날 때 
		for(int i = 0; i < 5; ++i) { 
			for(int j = 0; j < 3; j++) {
				for(int k = 0; k < 3; k++)
				System.out.println(i + "," + j + "," + k);
			}
			
		}
		System.out.println("-----------------------------");
	}
	
}
