	public static void main(String args[]) throws Exception
	{
		System.setIn(new FileInputStream("res/input_D2_1959.txt")); //제출할때는 입력 삭제!!!
		Scanner sc = new Scanner(System.in);
		int T=sc.nextInt(); 
		for(int tc = 1; tc <= T; tc++)
		{
			int N=sc.nextInt(); 
			int M=sc.nextInt(); 
			int Big;
			int[] A = new int[N];
			int[] B = new int[M];
			
			for(int i=0; i<N; i++) {
				A[i] =sc.nextInt();
			} 
			for(int j=0; j<M; j++) {
				B[j] =sc.nextInt();
			} 
		
			////
			if(N>M) Big=N;
			else Big= M;
			
			for(int i=0; i<Big; i++) {
				for 
			}

			System.out.print("#");
			/////////////////////////////////////////////////////////////////////////////////////////////

		}
		sc.close();
	}
}
