package array2;

public class SubSet1 {
	public static int[] a = {1,2,3,4};
	public static int[] bit = new int[4];
	public static void main(String[] args) {
		for(int i=0; i<2; i++) {
			bit[0] = (i%2==0)? 0:1;
			for(int j=0; j<2; j++) {
				bit[1] = (j%2==0)? 0:1;
				for(int k=0; k<2; k++) {
					bit[2] = (k%2==0)? 0:1;
				}
			}
		}
	}

}
