package array1;

public class 팁1289 {

	public static void main(String[] args) {
		String line = sc.next();
		char[] chs = line.toCharArray(); //로 스트링을 캐릭터의 배열로
		sysout(Arrays.toString(chs)); //, 다시 스트링으로 출력할수 있음.
		char cur = '0';
		int count = 0 ;
		for(int i=0; i<line.length(); i++) {
			char ch=line.charAt(i); //charAt함수
			if(ch!=cur) {
				count++;
				cur=ch;
			}
		}
		System.out.println("");
	}

}
